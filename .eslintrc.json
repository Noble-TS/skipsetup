{
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }],
    "no-redeclare": "off",  // Disable for overloads
    "@typescript-eslint/no-redeclare": "error"  // Use TS-aware version if plugin installed
  },
 
  "overrides": [
    {
      "files": ["src/utils/file.ts"],
      "rules": {
        "no-redeclare": "off",
        "no-unused-vars": "off",
        "@typescript-eslint/no-redeclare": "off",
        "@typescript-eslint/no-unused-vars": "off"
      }
    }
  ]

}